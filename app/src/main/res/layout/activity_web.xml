<?xml version="1.0" encoding="utf-8"?>
<com.zev.wanandroid.mvp.ui.view.LikeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/likeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mvp.ui.activity.WebActivity">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/flParent"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <view
            android:id="@+id/pbLoad"
            class="mbanje.kurt.fabbutton.FabButton"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_gravity="bottom"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="50dp"
            android:color="@color/white"
            android:indeterminate="false"
            android:max="100"
            android:src="@drawable/pb_left_arrow"
            app:fbb_autoStart="true"
            app:fbb_endBitmap="@drawable/pb_left_arrow"
            app:fbb_hideProgressOnComplete="true"
            app:fbb_progressColor="@color/color_433dee"
            app:fbb_progressWidthRatio="0.1"
            app:fbb_showEndBitmap="true"
            app:fbb_showShadow="true" />
    </FrameLayout>


</com.zev.wanandroid.mvp.ui.view.LikeLayout>